<?php
// $Id: custom_form.install

/**
* Implementation of hook_install()
*/
function custom_form_install() {
  // Set the module weight so it can override other modules.
  db_query("UPDATE {system} SET weight = 99 WHERE name = 'custom_form'");
}

/**
* Implementation of hook_uninstall()
*/
function custom_form_uninstall() {
  // Remove the module from the system table
  db_query("DELETE FROM {system} WHERE name = 'custom_form'");
}